package sk.idm.nssync.model;

import java.util.HashMap;
import java.util.Map;

public class UpdateInfo {
    public Map<String, String> id;
    public String tbl;

    public UpdateInfo() {
        this(null, null);
    }
    
    public UpdateInfo(int size) {
        id = new HashMap<>(size);
    }

    public UpdateInfo(Map<String, String> id, String tbl) {
        this.id = id;
        this.tbl = tbl;
    }
}
