package sk.idm.nssync;

/**
 *
 * @author mhajducek
 */
public class NSSyncException extends Exception{

    public NSSyncException() {
    }

    public NSSyncException(String message) {
        super(message);
    }

    public NSSyncException(String message, Throwable cause) {
        super(message, cause);
    }
    
}
